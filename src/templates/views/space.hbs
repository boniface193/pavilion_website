<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js">
<!--<![endif]-->

{{!-- css header files --}}
{{>header}}

<body>
    <!--[if lt IE 9]>
    		<div class="bg-danger text-center">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/" class="color-main">upgrade your browser</a> to improve your experience.</div>
    	<![endif]-->

    <div class="preloader text-center" style="padding-top: 50vh;">
        <div class="spinner-grow text-primary"></div>
    </div>

    <!-- wrappers for visual page editor and boxed version of template -->
    <div id="canvas">
        <div id="box_wrapper">

            {{!-- topbar menus --}}
            {{>topbar}}

            <div class="header_absolute header_layout_1">
                <!-- header with two Bootstrap columns - left for logo and right for navigation and includes (search, social icons, additional links and buttons etc -->
                <header class="page_header ls justify-nav-end">
                    <div class="container-fluid">
                        <div class=" justify-content-center">
                            <div class="col-xl-10 col-lg-10 col-md-10 col-1">
                                <div class="nav-wrap">

                                    <!-- main nav start -->
                                    <nav class="top-nav">
                                        <ul class="nav sf-menu">

                                            <li>
                                                <a href="/">Home</a>
                                            </li>

                                            <li>
                                                <a href="/about">About Us</a>
                                            </li>
                                            <!-- eof pages -->

                                            <li class="active">
                                                <a href="/space">Co-workingSpace</a>
                                            </li>

                                            <li>
                                                <a href="/academy">I/O Academy</a>
                                            </li>

                                            <li>
                                                <a href="/contact">Contact Us</a>
                                            </li>
                                        </ul>

                                    </nav>
                                    <!-- eof main nav -->
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- header toggler -->
                    <span class="toggle_menu position-fixed" style="top: 100px"><span></span></span>
                </header>
            </div>

            <section class="page_title cs s-pt-60 s-pb-10 s-pt-lg-130 s-pb-lg-60 page_title text-center page_slider">
                <div id="particles-js"></div>
                <img src="image/slide.jpg" alt="" style="height: 55vh;">
                <div class="container-fluid">
                    <div class="row">
                        <div class="divider-60"></div>
                        <div class="col-md-12 text-center">
                            <h1>Co-working Space</h1>
                        </div>
                        <div class="divider-50"></div>
                    </div>
                </div>
                <ol class="breadcrumb full-width bg-maincolor">
                    <li class="breadcrumb-item">
                        <a href="/">Home</a>
                    </li>
                    <li class="breadcrumb-item active">
                        Space
                    </li>
                </ol>
            </section>

            {{!-- Pricing --}}
            <section class="s-py-80 s-py-lg-120 s-py-xl-160 ls ms">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-8 offset-lg-2">
                            <h5 class="special-heading text-center">Choose</h5>
                            <h1 class="text-center">Package</h1>
                            <p class="text-center big">We offer an affordable alternative to renting or buying of office
                                space.
                                you get a secure and professional turn-key office with all the benefit of a large
                                office.</p>
                        </div>
                    </div>
                    <div class="divider-45 d-none d-lg-block"></div>
                    <div class="row c-gutter-60">
                        {{#each pricing}}
                        <div class="col-lg-4 col-12 mt-5">
                            <div class="pricing-plan hero-bg rounded h-100">
                                <div class="plan-name text-uppercase bg-maincolor">
                                    <h3>
                                        {{planName}}
                                    </h3>
                                </div>
                                <div class="price-wrap d-flex">
                                    <span class="plan-sign small-text">₦</span>
                                    <span class="plan-price color-main2">{{price}}</span>
                                    <span class="plan-decimals small-text">{{span}}</span>
                                </div>
                                <div class="plan-features">
                                    <ul class="list-bordered">
                                        {{#each planDetails}}
                                        <li>{{planInfo}}</li>
                                        {{/each}}
                                    </ul>
                                </div>
                            </div>
                            <div class="divider-45 d-block d-lg-none"></div>
                        </div>
                        {{/each}}
                        <div class="divider-20 d-block d-md-none"></div>
                    </div>
                    <div class="plan-button mt-5 text-center">
                        <a href="/contact" class="btn btn-maincolor2 btn-lg">Get in touch</a>
                    </div>
                </div>
            </section>
            {{!-- pricing end here --}}

            <section class="s-pt-75 s-pb-60 s-pb-md-80 s-py-lg-120 s-pt-xl-155 s-pb-xl-160 ls bg-maincolor"
                id="testimonials">
                <div class="container">
                    <div class="text-center">
                        <h5 class="special-heading">Meet ups and</h5>
                        <h3 class="text-uppercase">Hangouts</h3>
                    </div>
                    <div class="d-block d-lg-none divider-5"></div>
                    <div class="row c-gutter-60 align-items-center">
                        <div class="col-xl-6 col-lg-6 col-lg-6 col-xs-12 order-2 order-lg-1 animate"
                            data-animation="fadeInLeftBig">
                            <div class="side-image">
                                <img src="services/share.svg" width="880" alt="">
                                <div class="d-block d-lg-none divider-10"></div>
                            </div>
                        </div>
                        <div class="col-xl-6 col-lg-6 col-lg-6 col-xs-12 order-1 order-lg-2">
                            <div class="item-content text-center text-lg-left">
                                <h3 class="display-4">₦15,000 for 5 hours pay day </h3>
                                <ul class="nav menu-click">
                                    <li><i class="fas fa-check text-danger mr-2"></i>with Photographer (₦20,000)</li>
                                    <li><i class="fas fa-check text-danger mr-2"></i>Unlimited Internet</li>
                                    <li><i class="fas fa-check text-danger mr-2"></i>24/7 Power Supply</li>
                                    <li><i class="fas fa-check text-danger mr-2"></i>After Hours Gaming Session</li>
                                </ul>
                                <a class="btn btn-maincolor2 mt-4" href="/contact"><span>Get in touch</span></a>
                                <div class="d-flex justify-content-center mt-50 d-none">

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            <section class="ls s-pt-80 s-pb-20 s-pt-lg-130 s-pb-lg-70 s-pt-xl-160 s-pb-xl-100 c-gutter-60 c-mb-50 "
                id="advantage">
                <div class="container">
                    <div class="text-center mb-5">
                        <h5 class="special-heading">Check out Our</h5>
                        <h3 class="text-uppercase">Students Plan</h3>
                        <p>Students who wish to take advantage of the student plan have to provide a valid student ID
                            Card</p>
                    </div>
                    <div class="row">
                        {{#each student}}
                        <div class="col-xl-3 col-md-6 animate" data-animation="fadeInUp">
                            <div class="icon-box text-center">
                                <h5>
                                    {{planName}}
                                </h5>
                                <div>
                                    <h6>₦{{price}}</h6>
                                    <ul class="list1">
                                        {{#each planDetails}}
                                        <li>{{planInfo}}</li>
                                        {{/each}}
                                    </ul>
                                </div>
                            </div>
                        </div>
                        {{/each}}
                        <div class="d-none d-lg-block divider-small"></div>
                    </div>
                    <div class="plan-button mt-5 text-center">
                        <a href="/contact" class="btn btn-maincolor2 btn-lg">Get in touch</a>
                    </div>
                </div>
            </section>

            {{!-- footers for our website --}}
            {{>footer}}
        </div><!-- eof #box_wrapper -->
    </div><!-- eof #canvas -->

    {{>scriptTemplate}}


</body>

</html>